body {
 display:block;
 /*background-color: rgba(255,255,255,0.9); */
 position: relative;
 font-family: "Helvetica Neue","Helvetica",Helvetica,Arial,sans-serif;
 font-weight: normal;
 font-style: normal;
 line-height: 1;
  padding: 0;
  margin: 0;
}

body:after {
    content: "";
    background: url('../images/atomiocbackground.jpg') repeat;
    opacity: 0.8;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    position: absolute;
    z-index: -1;
}

#page {
  max-width: 1040px;
  margin: 0 auto;
}

/*Container*/

.container {
  max-width:1040px;
  background: rgba(255,255,255, 1);
  position: relative;
  @include clearfix; 
  margin: 0 auto;
}

.container, header#main-header, footer#main-footer {
  -webkit-box-shadow: 1px 1px 2px 2px #ccc;
  box-shadow: 1px 1px 2px 2px #ccc;
}

#main-container {
  width: 100%; 
  overflow: hidden; 
}

button.secondary, .button.secondary {
  background-color: #fff;
  color: #09021d;
  text-transform: uppercase;
  font-size: .65em;
  border-color: #09021d;
}

button.secondary:hover, .button.secondary:hover {
  background: #c01313;
  color: #fff;
}

/*HEADER*/

header#main-header {
  max-width: 1040px;
  background: #fff;
  opacity: 1;
  @include clearfix; 
  margin: 0 auto;
}


#minehead-image {
  background: url('../images/headframe.png') top left no-repeat;
  height: 170px;
  width: 18%;
  float: left;
  margin-top: 40px; 
  margin-left: 25px;
}

.title-block {
  width: 64%;
  float: left;
  margin-top: 10px;
  margin-left: -20px;
}

#atomic-symbol {
  background: url('../images/atomic-symbol.png') top left no-repeat;
  height: 170px;
  width: 16%;
  float: left;
  padding-right: 0;
}

nav.search-bar{
  width: 50%;
  float: right;
  /*background: #ccc; */
  height: 35px;
  margin: 10px 0;
}

nav.search-bar form {
  width: 300px;
  float: right;
}

nav.search-bar .button {
  background-color: #09021d;
  border-color: #09021d;
}

nav.search-form #searchform div {
  margin: 0;
}
nav.search-form #s {
  float: right;
  -webkit-transition-duration: 400ms;
  -webkit-transition-property: width, background;
  -webkit-transition-timing-function: ease;
  -moz-transition-duration: 400ms;
  -moz-transition-property: width, background;
  -moz-transition-timing-function: ease;
  -o-transition-duration: 400ms;
  -o-transition-property: width, background;
  -o-transition-timing-function: ease;
  width: auto;
}
nav.search-form #s:focus {
  background-color: #f9f9f9;
  width: 196px;
}
nav.search-form #searchsubmit {
  display: none;
}
nav.search-form .only-search #searchform {
  top: 5px;
  z-index: 1;
}
nav.search-form .only-search #s {
  background-color: #666;
  border-color: #000;
  color: #222;
}
nav.search-form .only-search #s,
nav.search-form .only-search #s:focus {
  width: 85%;
}
nav.search-form .only-search #s:focus {
  background-color: #bbb;
}
nav.search-form .with-image #searchform {
  top: auto;
  bottom: -27px;
  max-width: 195px;
}
nav.search-form .only-search + #access div {
  padding-right: 205px;
}


/* -----------------------------------------
   HOME
----------------------------------------- */

/*Top Section*/

section#main-content {
  overflow: hidden;
  width: 64%;
  float: left;  
  padding: 5px 15px;
}

#feature-slider {
  width: 100%;
  padding: 0; 
}

#feature-slider img {
  width: 100%;
}

#main-content header h1 {
  background: #09021d;
  padding: 5px 8px;
  font-size: 16px;
  color: #fff;
  a {
  color: #fff; 
  }
}

#latest-posts {
  width: 100%;
  figure {
  width: 45%; 
  float: left; 
  }
}

 #latest-posts h3 {
  margin-top: 10px;
  margin-bottom: 5px;
  a {
  font-size: 1.5em; 
  }
}

article#latest-posts h2 a {
  font-size: 18px;
}

.latest-text {
  width: 50%; 
  float: right; 
  padding: 0 1%; 

}

article.feature-content {
  float: left;
}

/*Bottom Content*/

#bottom-container {
  float: left;
  width: 100%;
  border-top: 5px solid #ccc;
}

.bottom-content {
  width: 100%;
  float: left;
}

section.bottom-content header h1 a {
  color: #fff;
  text-align: center;
}

section.top-boxes {
  float: left;
  width: 100%;
  padding-bottom: 10px;
  margin-top: 10px; 
  margin-bottom: 15px;
  margin-left: 0px;
  border-bottom: 2px solid #ccc;
  .articles {
  width: 48%; 
  float: left; 
  padding: 0; 
  margin: 0; 
  }
  .articles:first-child {
  margin-right: 4%; 
  }
}

.article-title {
  height: 30px; 
  margin: 15px 0 10px 0; 
}

header.article-header {
  height: 120px;
}

h3.article-title {
  height: 40px;
}

.article-text {
  width: 96%;
}

.bottom-box {
  width: 31%;
  float: left;
  padding: 0;
  margin-right: 3.2%;
}

article.bottom-box:last-child {
  margin-right: 0px;
  border-right: 1px solid #fff;
}

section.bottom-boxes {
  padding-left: 0px !important;
  margin-bottom: 15px;
   h1 {
  font-size: 16px;
  font-weight: 500;
  }
  img {
  width: 98%
  }
}


/* -----------------------------------------
   PAGES
----------------------------------------- */

#primary {
  width: 64%; 
  float: left; 
  padding: 0 15px; 
  margin-top: 40px; 
  margin-bottom: 50px; 
}

.archive-content, .single-content {
  width: 66%;
  float: left;
}

.entry-content {
  float: left;
  h1 {
  font-family: 'Arial Black', Arial, sans-serif; 
  }
  h1 a {
  font-family: "American Typewriter", MonospaceTypewriterRegular, monospace;
  font-size: 24px;
  font-weight: 400;
  }
}

.archive-image {
  float: left;
  width: 210px;
  img {
  padding: 2px;
  border: 1px solid #09021d;
  }
}

.archive-text {
  width: 65%;
  float: right;
}

.archive-text button.secondary, .archive-text .button.secondary {
  margin-top: 20px;
}

/* -----------------------------------------
   SINGLE
----------------------------------------- */

.single-content {
  margin-top: 40px; 
  overflow: hidden; 
}

.single-content article {
  padding: 0 25px; 
  header {
  width: 100%; 
  }
  h2 a {
  font-size: 1.5em; 
  }
}
/* -----------------------------------------
   ARCHIVE PAGES
----------------------------------------- */

header.archives {
  margin: 15px 0 15px 10px;
}

.archive-content {
  margin: 15px 0;
}

.entry-content {
  float: left;
  height: auto;
  width: 100%;
  padding-bottom: 18px;
  border-bottom: 1px solid $black;
}

#gallery-page {
  overflow: hidden;
}

#gallery-text {
  float: left;
  width: 33%;
}

#gallery {
  float: right;
  width: 66%;
}

li.gallery-images {
  width: 50%;
  float: left;
}

li.gallery-images div > p.wp-caption-text {
  display: none;
}

li.gallery-images:hover div > p.wp-caption-text {
  display: block;
}

/*.wp-caption-text {
  text-align: center;
  position: absolute;
  width: 80%;
  height: auto;
  color: #09021d;
  background: rgba(255,255,255,0.5);
  font-size: 1em;
  vertical-align: middle;
  left: 10%;
  padding: 5%;
  border: 1px solid #fff;
}*/

.archive-thumbnail {
  width: 33%;
  float: left;
}

.archive-text {
  width: 66%;
  float: right;
}

#photo-gallery {
  width: 100%;
  background: #fff;
}

article.photo-thumbnails {
  width: 100%;
  float: left;
}

.gallery {
  float: right;
  width: 66%;
}

article.wall-thumbnails {
  height: 80%;
}

#photo-wall article.photo-thumbnail .photo-gallery {
  width: 100%;
}

.wall-thumbnail {
  width: 100%;
}

div.photo-text {
  width: 100%;
  /*float: left;*/
  margin-top: 10px;
}

p.item {
  margin: 5px auto;
}

/* -----------------------------------------
   VIDEO PAGES
----------------------------------------- */

#video .entry-content {
  border-bottom: none;
}

.video-thumbnail {
  float: left;
  width: 200px;
  margin-right: 10px;
}

.video-thumbnail h2 {
  text-align: center;
  line-height: 1em;
}

.video-thumbnail h2 a {
  font-size: 14px;
}

/* -----------------------------------------
   COMMENT
----------------------------------------- */

#respond {
  background: $grey; 
  width: 90%;
  float: left;  
  padding: 15px 25px; 
  margin-left: 10px; 
  .form-allowed-tags {
  display: none; 
  }
  text-area {
  width: 95%; 
  margin: 0 auto; 
  }
  border-radius: 5px; 
}

/* -----------------------------------------
   SIDEBAR
----------------------------------------- */

.home-sidebar {
  width: 30%;
  float: right;
  padding-left: 1%; 
  margin-right: 15px; 
  border-left: 1px solid $grey; 
  ul {
  margin: 0; 
  padding: 0; 
  }
}

.sidebar {
  @extend .home-sidebar; 
  border-left: none; 
  margin-top: 50px; 
}

.inside-box {
  width: 25%;
  float: left;
}

.uc-pin {
  background: url('../images/uc-pin.png') top left no-repeat;
  width: 78px;
  height: 80px;
  margin: 10px auto 10px auto;
}

.sidebar-header, button {
  height: 35px;
  background: #c01313; /* Old browsers */
background: -moz-linear-gradient(top,  #c01313 0%, #c01313 70%, #09021d 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#c01313), color-stop(70%,#c01313), color-stop(100%,#09021d)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* IE10+ */
background: linear-gradient(to bottom,  #c01313 0%,#c01313 70%,#09021d 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c01313', endColorstr='#09021d',GradientType=0 ); /* IE6-9 */

  width: 100%;
  text-align: center;
  padding-top: 8px;
  margin-bottom: 5px;
  h1 {
  font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
  font-weight: 100;
  font-size: 18px;
  line-height: .8;
}

}

button {
  width: auto;
}

header.sidebar-header h1 a {
  color: #fff;
  font-size: 14px;
  line-height: 80%;
}

.home-sidebar figure img {
  text-align: center;
  max-width: 98%; 
  padding: 2px;
  border: 1px solid #09021d;
}

aside.sidebar h5 {
  color: #fff;
  text-transform: uppercase;
  text-align: center;
  background: #c01313; /* Old browsers */
  background: -moz-linear-gradient(top,  #c01313 0%, #c01313 70%, #09021d 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#c01313), color-stop(70%,#c01313), color-stop(100%,#09021d)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* IE10+ */
  background: linear-gradient(to bottom,  #c01313 0%,#c01313 70%,#09021d 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c01313', endColorstr='#09021d',GradientType=0 ); /* IE6-9 */
  padding: 5px 10px;
}

.recent-posts {
  overflow: hidden;
  margin-bottom: 10px; 
  .recent-posts-thumbnail {
  float: left;
  width: 35%;
  }
  .recent-posts-title {
  float: right;
  width: 60%;
  padding-left: 2%;
  }
}

aside.sidebar h4, aside.home-sidebar h4 {
  font-size: 1em;
  padding: 0; 
  margin: 0; 
  a {
  margin: 0; 
  padding: 0; 
  }
}

/* -----------------------------------------
   SUB FOOTER
----------------------------------------- */

section#inside-carousel {
  overflow: hidden; 
  border: 1px solid #09021d;
}

section.inside-boxes {
  padding: 0 10px;
}

header.inside {
  padding: 10px 0px 5px 15px;
  border-bottom: 4px double #09021d;
}

header.inside h1 {
  font-family: 'a_love_of_thunderregular', sans-serif;
}

.bx-wrapper img {
  padding: 2px;
  border: 1px solid #09021d;
}

/* -----------------------------------------
   FOOTER
----------------------------------------- */

footer#main-footer {
  width: 1040px;
  background: #09021d;
  color: #fff;
  overflow:hidden;
  padding: 15px 25px;
  margin: 0 auto;
}

footer ul ul {
  float: left;
  width: 25%;
}

footer ul ul.uc-links {
  width: 50%;
}

footer#main-footer h5 {
  color: #fff;
  font-size: 12px;
  text-transform: uppercase;
