.archive-thumbnail {
  width: 33%;
  float: left;
}

.archive-text {
  width: 66%;
  float: right;
}

/* -----------------------------------------
   PHOTO PAGES
----------------------------------------- */

#photo-gallery {
  width: 100%;
  background: #fff;
}

article.photo-thumbnails {
  width: 100%;
  float: left;
}

article.wall-thumbnails {
  height: 80%;
}

.wall-thumbnail {
  width: 100%;
}

.photo-text {
  width: 31%;
  float: left;
  padding: 0 1%;
}

.photo-gallery {
  width: 66%;
  float:right; 
}

p.item {
  margin: 5px auto;
}

/* -----------------------------------------
   VIDEO PAGES
----------------------------------------- */

#video .entry-content {
  border-bottom: none;
}

.video-thumbnail {
  float: left;
  width: 200px;
  margin-right: 10px;
}

.video-thumbnail h2 {
  text-align: center;
  line-height: 1em;
}

.video-thumbnail h2 a {
  font-size: 14px;
}

body.single-memorabilia .single-content {
  width: 100%; 
}

.intro {
  width: 25%; 
  float: left; 
  margin-bottom: 2em; 
}

.intro-text {
  width: 100%; 
  float: left; 
}

.intro-photo {
  width: 100%; 
  float: right; 
}

/* ---- masonry ---- */

.memorabilia-gallery {
  width: 73%;
  float: right; 
  position: relative;  
  overflow:hidden; 
}

/* clear fix */
.memorabilia-gallery:after {
  content: '';
  display: block;
  clear: both;
}

/* ---- .item ---- */

.single-memorabilia .item {
  float: left;
  width: 30%;
  height: auto;
  background: transparent; 
  padding: 2px; 
  margin: 0;
}

.item img {
  border: none;
}

.item.width2 { width: 60%; }
.item.height2 { height: 300px; }

/* -----------------------------------------
   PLACES
----------------------------------------- */

.content-container {
      width: 100%; 
  overflow: hidden;
    position: relative;
}

#single-places-content {
  width: 100%; 
  overflow: hidden;
  position: relative;
}

#places-header {
  position: absolute;
  left: 2%; 
  top: 3%; 
}

#places-header h1 {
  color: #fff;
  font-size: 36px;
  font-weight: 600;
  opacity: 1;
}

.video-container {
  width: 90%;
  position: relative;
  padding: 1%;
  margin: 0em 5% 2em 6%;
}

