  width: 100%;
  text-align: center;
  padding-top: 8px;
  margin-bottom: 5px;
  h1 {
  font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
  font-weight: 100;
  font-size: 18px;
  line-height: .8;
}

}

button {
  width: auto;
}

header.sidebar-header h1 a {
  color: #fff;
  font-size: 14px;
  line-height: 80%;
}

.home-sidebar figure img {
  text-align: center;
  max-width: 98%; 
  padding: 2px;
  border: 1px solid #09021d;
}

aside.sidebar h5 {
  color: #fff;
  text-transform: uppercase;
  text-align: center;
  background: #c01313; /* Old browsers */
  background: -moz-linear-gradient(top,  #c01313 0%, #c01313 70%, #09021d 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#c01313), color-stop(70%,#c01313), color-stop(100%,#09021d)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* IE10+ */
  background: linear-gradient(to bottom,  #c01313 0%,#c01313 70%,#09021d 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c01313', endColorstr='#09021d',GradientType=0 ); /* IE6-9 */
  padding: 5px 10px;
}

.recent-posts {
  overflow: hidden;
  margin-bottom: 10px; 
  .recent-posts-thumbnail {
  float: left;
  width: 35%;
  }
  .recent-posts-title {
  float: right;
  width: 60%;
  padding-left: 2%;
  }
}

aside.sidebar h4, aside.home-sidebar h4 {
  font-size: 1em;
  padding: 0; 
  margin: 0; 
  a {
  margin: 0; 
  padding: 0; 
  }
}

/* -----------------------------------------
   SUB FOOTER
----------------------------------------- */

section#inside-carousel {
  overflow: hidden; 
  border: 1px solid #09021d;
}

section.inside-boxes {
  padding: 0 10px;
}

header.inside {
  padding: 10px 0px 5px 15px;
  border-bottom: 4px double #09021d;
}

header.inside h1 {
  font-family: 'a_love_of_thunderregular', sans-serif;
}

.bx-wrapper img {
  padding: 2px;
  border: 1px solid #09021d;
}

/* -----------------------------------------
   FOOTER
----------------------------------------- */

footer#main-footer {
  width: 1040px;
  background: #09021d;
  color: #fff;
  overflow:hidden;
  margin: 0 auto;
}

.footer-links {
  width: 98%; 
  overflow: hidden; 
  margin: 0 auto; 
}

footer ul ul {
  float: left;
  width: 25%;
}

footer ul ul.uc-links {
  width: 50%;
}

footer#main-footer h5 {
  color: #fff;
  font-size: 12px;
  text-transform: uppercase;
