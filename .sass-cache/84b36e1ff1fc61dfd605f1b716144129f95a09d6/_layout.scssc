.places-main-content {
  float: left;
  width: 100%;
  /*margin-left: 2%;*/
}

#places-content {
  width: 98%;
  float: left;
  margin: 1em 1% 2em 1%;
}

#places-content h1 {
  font-family: "Arial Black", Arial, sans-serif;
  font-size: 2.4em; 
  font-weight: 600;
}

#places-content p {
  width:66%;
}

#places-content .pdf {
  width: 20%; 
  float: right;
  margin-left: 1.5%;
  margin-right: 12%;
}

#places-content img {
  border: none !important;
}

#places-content .wp-credit {
  font-size: .65em;
  line-height: 1;
}

#places-content .image-gallery, #places-content .link, #places-content .single-image {
  width: 33%;
  float: right;
  margin: 0 0% 0 1.5%;
}

#places-content .image-gallery #gallery-2 img {
  border: none;
}

#places-content dl.gallery-item {
  line-height: 1;
  margin-top: 0px;
  margin-bottom: 0px;
}

#places-content .gallery {
  width: 100%;
}

#places-content p.first-paragraph:first-letter {
  font-weight: 500;
  float: left;
  line-height: .88;
  font-size: 7em;
  text-indent: 0;
  margin-right: 6px;
}

#places-content p.first-paragraph:first-line {
  font-variant: small-caps;
  text-transform: uppercase;
  letter-spacing: .1em;
  font-size: 1.2em
}


/* -----------------------------------------
   SUB FOOTER
----------------------------------------- */

section#inside-carousel {
  overflow: hidden; 
  border: 1px solid #09021d;
}

section.inside-boxes {
  padding: 0 10px;
}

header.inside {
  padding: 10px 0px 5px 15px;
  border-bottom: 4px double #09021d;
}

header.inside h1 {
  font-family: 'a_love_of_thunderregular', sans-serif;
}

.bx-wrapper img {
  padding: 2px;
  border: 1px solid #09021d;
}

/* -----------------------------------------
   FOOTER
----------------------------------------- */

footer#main-footer {
  width: 1040px;
  background: #09021d;
  color: #fff;
  overflow:hidden;
  margin: 0 auto;
}

.footer-links {
  width: 98%; 
  overflow: hidden; 
  margin: 0 auto; 
}

footer ul ul {
  float: left;
  width: 25%;
}

footer ul ul.uc-links {
  width: 50%;
}

footer#main-footer h5 {
  color: #fff;
  font-size: 12px;
  text-transform: uppercase;
