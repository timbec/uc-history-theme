body {
 display:block;
 /*background-color: rgba(255,255,255,0.9); */
 position: relative;
 font-family: "Helvetica Neue","Helvetica",Helvetica,Arial,sans-serif;
 font-weight: normal;
 font-style: normal;
 line-height: 1;
  padding: 0;
  margin: 0;
}

body:after {
    content: "";
    background: url('images/atomiocbackground.jpg') repeat;
    opacity: 0.8;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    position: absolute;
    z-index: -1;
}

#page {
  max-width: 1040px;
  margin: 0 auto;
}

/*Container*/

.container {
  max-width:1040px;
  background: rgba(255,255,255, 1);
  position: relative;
  @include clearfix; 
  margin: 0 auto;
}

.container, header#main-header, footer#main-footer {
  -webkit-box-shadow: 1px 1px 2px 2px #ccc;
  box-shadow: 1px 1px 2px 2px #ccc;
}

#main-container {
  width: 100%; 
  overflow: hidden; 
}

button.secondary, .button.secondary {
  background-color: #fff;
  color: #09021d;
  text-transform: uppercase;
  font-size: .65em;
  border-color: #09021d;
}

button.secondary:hover, .button.secondary:hover {
  background: #c01313;
  color: #fff;
}

/*HEADER*/

header#main-header {
  max-width: 1040px;
  background: #fff;
  opacity: 1;
  @include clearfix; 
  margin: 0 auto;
}


#minehead-image {
  background: url('images/headframe.png') top left no-repeat;
  height: 170px;
  width: 18%;
  float: left;
  margin-top: 40px; 
  margin-left: 25px;
}

.title-block {
  width: 64%;
  float: left;
  margin-top: 10px;
  margin-left: -20px;
}

#atomic-symbol {
  background: url('images/atomic-symbol.png') top left no-repeat;
  height: 170px;
  width: 16%;
  float: left;
  padding-right: 0;
}

nav.search-bar{
  width: 50%;
  float: right;
  /*background: #ccc; */
  height: 35px;
  margin: 10px 0;
}

nav.search-bar form {
  width: 300px;
  float: right;
}

nav.search-bar .button {
  background-color: #09021d;
  border-color: #09021d;
}

nav.search-form #searchform div {
  margin: 0;
}
nav.search-form #s {
  float: right;
  -webkit-transition-duration: 400ms;
  -webkit-transition-property: width, background;
  -webkit-transition-timing-function: ease;
  -moz-transition-duration: 400ms;
  -moz-transition-property: width, background;
  -moz-transition-timing-function: ease;
  -o-transition-duration: 400ms;
  -o-transition-property: width, background;
  -o-transition-timing-function: ease;
  width: auto;
}
nav.search-form #s:focus {
  background-color: #f9f9f9;
  width: 196px;
}
nav.search-form #searchsubmit {
  display: none;
}
nav.search-form .only-search #searchform {
  top: 5px;
  z-index: 1;
}
nav.search-form .only-search #s {
  background-color: #666;
  border-color: #000;
  color: #222;
}
nav.search-form .only-search #s,
nav.search-form .only-search #s:focus {
  width: 85%;
}
nav.search-form .only-search #s:focus {
  background-color: #bbb;
}
nav.search-form .with-image #searchform {
  top: auto;
  bottom: -27px;
  max-width: 195px;
}
nav.search-form .only-search + #access div {
  padding-right: 205px;
}


/* -----------------------------------------
   HOME
----------------------------------------- */

/*Top Section*/

#main-content {
  overflow: hidden;
  width: 64%;
  float: left;  
  padding: 5px 15px;
}

#feature-slider {
  width: 100%;
  padding: 0; 
}

#feature-slider img {
  width: 100%;
}

#main-content header h1 {
  background: #09021d;
  padding: 5px 8px;
  font-size: 16px;
  color: #fff;
  a {
  color: #fff; 
  }
}

#latest-posts {
  width: 100%;
  figure {
  width: 45%; 
  float: left; 
  }
}

 #latest-posts h3 {
  margin-top: 10px;
  margin-bottom: 5px;
  a {
  font-size: 1.5em; 
  }
}

article#latest-posts h2 a {
  font-size: 18px;
}

.latest-text {
  width: 50%; 
  float: right; 
  padding: 0 1%; 

}

article.feature-content {
  float: left;
}

/*Bottom Content*/

#bottom-container {
  float: left;
  width: 100%;
  border-top: 5px solid #ccc;
}

.bottom-content {
  width: 100%;
  float: left;
}

section.bottom-content header h1 a {
  color: #fff;
  text-align: center;
}

section.top-boxes {
  float: left;
  width: 100%;
  padding-bottom: 10px;
  margin-top: 10px; 
  margin-bottom: 15px;
  margin-left: 0px;
  border-bottom: 2px solid #ccc;
  .articles {
  width: 48%; 
  float: left; 
  padding: 0; 
  margin: 0; 
  }
  .articles:first-child {
  margin-right: 4%; 
  }
}

.article-title {
  height: 30px; 
  margin: 15px 0 10px 0; 
}

header.article-header {
  height: 120px;
}

h3.article-title {
  height: 40px;
}

.article-text {
  width: 100%;
}

.bottom-box {
  width: 31%;
  float: left;
  padding: 0;
  margin-right: 3.2%;
}

article.bottom-box:last-child {
  margin-right: 0px;
  border-right: 1px solid #fff;
}

section.bottom-boxes {
  padding-left: 0px !important;
  margin-bottom: 15px;
   h1 {
  font-size: 16px;
  font-weight: 500;
  }
  img {
  width: 98%
  }
}


/* -----------------------------------------
   PAGES
----------------------------------------- */

#primary {
  width: 64%; 
  float: left; 
  padding: 0 15px; 
  margin-top: 40px; 
  margin-bottom: 50px; 
}

.archive-content {
  width: 66%;
  float: left;
}

.entry-content {
  float: left;
  h1 {
  font-family: 'Arial Black', Arial, sans-serif; 
  }
  h1 a {
  font-family: "American Typewriter", MonospaceTypewriterRegular, monospace;
  font-size: 24px;
  font-weight: 400;
  }
}

.archive-image {
  float: left;
  width: 210px;
  img {
  padding: 2px;
  border: 1px solid #09021d;
  }
}

.archive-text {
  width: 65%;
  float: right;
}

.archive-text button.secondary, .archive-text .button.secondary {
  margin-top: 20px;
}

/* -----------------------------------------
   SINGLE
----------------------------------------- */

.single-content {
  width: 66%; 
  float: left; 
  margin-top: 40px; 
  overflow: hidden; 
}

.single-content article {
  padding: 0 25px; 
  header {
  width: 100%; 
  }
  h2 a {
  font-size: 1.5em; 
  }
}


/* -----------------------------------------
   ARCHIVE PAGES
----------------------------------------- */

header.archives {
  margin: 15px 0 15px 10px;
}

.archive-content {
  margin: 15px 0;
}

.entry-content {
  float: left;
  height: auto;
  width: 100%;
  padding-bottom: 18px;
  border-bottom: 1px solid $black;
}

#gallery-page {
  overflow: hidden;
}

#gallery-text {
  float: left;
  width: 33%;
}

#gallery {
  float: right;
  width: 66%;
}

li.gallery-images {
  width: 50%;
  float: left;
}

li.gallery-images div > p.wp-caption-text {
  display: none;
}

li.gallery-images:hover div > p.wp-caption-text {
  display: block;
}

