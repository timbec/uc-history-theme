.places-main-content {
  float: left;
  width: 100%;
  /*margin-left: 2%;*/
}

#places-content {
  width: 98%;
  float: left;
  margin: 1em 1% 2em 1%;
}

#places-content h1 {
  font-family: "Arial Black", Arial, sans-serif;
  font-size: 2.4em; 
  font-weight: 600;
}

#places-content p {
  width:66%;
}

#places-content .pdf {
  width: 20%; 
  float: right;
  margin-left: 1.5%;
  margin-right: 12%;
}

#places-content img {
  border: none !important;
}

#places-content .wp-credit {
  font-size: .65em;
  line-height: 1;
}

#places-content .image-gallery, #places-content .link, #places-content .single-image {
  width: 33%;
  float: right;
  margin: 0 0% 0 1.5%;
}

#places-content .image-gallery #gallery-2 img {
  border: none;
}

#places-content dl.gallery-item {
  line-height: 1;
  margin-top: 0px;
  margin-bottom: 0px;
}

#places-content .gallery {
  width: 100%;
}

#places-content p.first-paragraph:first-letter {
  font-weight: 500;
  float: left;
  line-height: .88;
  font-size: 7em;
  text-indent: 0;
  margin-right: 6px;
}

#places-content p.first-paragraph:first-line {
  font-variant: small-caps;
  text-transform: uppercase;
  letter-spacing: .1em;
  font-size: 1.2em
}

/* -----------------------------------------
   COMMENT
----------------------------------------- */

#respond {
  background: $grey; 
  width: 90%;
  float: left;  
  padding: 15px 25px; 
  margin-left: 10px; 
  .form-allowed-tags {
  display: none; 
  }
  text-area {
  width: 95%; 
  margin: 0 auto; 
  }
  border-radius: 5px; 
}

/* -----------------------------------------
   SIDEBAR
----------------------------------------- */

.home-sidebar {
  width: 30%;
  float: right;
  padding-left: 1%; 
  margin-right: 15px; 
  border-left: 1px solid $grey; 
  ul {
  margin: 0; 
  padding: 0; 
  }
}

.sidebar {
  @extend .home-sidebar; 
  border-left: none; 
  margin-top: 50px; 
}

.inside-box {
  width: 25%;
  float: left;
}

.uc-pin {
  background: url('images/uc-pin.png') top left no-repeat;
  width: 78px;
  height: 80px;
  margin: 10px auto 10px auto;
}

.sidebar-header, button {
  height: 35px;
  background: #c01313; /* Old browsers */
background: -moz-linear-gradient(top,  #c01313 0%, #c01313 70%, #09021d 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#c01313), color-stop(70%,#c01313), color-stop(100%,#09021d)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #c01313 0%,#c01313 70%,#09021d 100%); /* Chrome10+,Safari5.1+ */
